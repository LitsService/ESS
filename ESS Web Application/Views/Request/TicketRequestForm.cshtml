    function SelectTravelFromGetCityEdit(e,select) {
        var countryId = e;
        $("#TravelFromCityEdit").html("");
        $.ajax({
            type: "Post",
            url: "/Request/GetCities",
            data: { CountryId: countryId },
            success: function (data) {
                var Cities = '';
                for (var i = 0; i < data.length; i++) {
                    if (data[i].Value == select) {
                        Cities += '<option selected value="' + data[i].Value + '">' + data[i].Text + '</option>';
                    }
                    else {
                        Cities += '<option value="' + data[i].Value + '">' + data[i].Text + '</option>';
                    }
                }
                $("#TravelFromCityEdit").html(Cities);
            }
        });
    }

