@using (Html.BeginForm("DownloadReport", "Request", FormMethod.Post))
{
    <input type="hidden" name="PrintId" />
    <input type="submit" class="hidden" hidden id="btnSubmit" value="Export" />
}
<script>

    $(document).ready(function () {
        $("#User").trigger("change");
        BindDataTable();
    });

    function Attachment() {
        $("#file").click();
    }
    function Download(id) {
        $("#PrintId").val(id);
        $("#btnSubmit").click();
    }

    function GetEmployeeDetails(e) {
        $.ajax({
            type: "Post",
            url: "/Request/GetEmployeeDetails",
            data: { SelectUserId: e.value },
            success: function (data) {
                $("#EmpName").text(data.Name);
                $("#EmpDesg").text(data.Designation);
                $("#EmpID").text(data.EmpID);
                $("#EmpNation").text(data.Nationality);
                $("#EmpDept").text(data.Department);
                $("#EmpAdd").text(data.Address);
                $("#EmpPhone").text(data.Phone);
                $("#EmpEmail").text(data.Email);
            }
        });
    }

    function TravelFromGetCity(e) {
        var countryId = e.value;
        $("#TravelFromCity").html("");
        $.ajax({
            type: "Post",
            url: "/Request/GetCities",
            data: { CountryId: countryId },
            success: function (data) {
                var Cities = '';
                for (var i = 0; i < data.length; i++) {
                    Cities += '<option value="' + data[i].Value + '">' + data[i].Text + '</option>';
                }
                $("#TravelFromCity").html(Cities);
            }
        });
    }

    function TravelToGetCity(e) {
        var countryId = e.value;
        $("#TravelToCity").html("");
        $.ajax({
            type: "Post",
            url: "/Request/GetCities",
            data: { CountryId: countryId },
            success: function (data) {
                var Cities = '';
                for (var i = 0; i < data.length; i++) {
                    Cities += '<option value="' + data[i].Value + '">' + data[i].Text + '</option>';
                }
                $("#TravelToCity").html(Cities);
            }
        });
    }

    function BindDataTable() {
        $.ajax({
            type: "Post",
            url: "/Request/GetTicketData",
            success: function (data) {
                $("#TicketHistory").DataTable({
                    "lengthChange": true,
                    "pageLength": 25,
                    destroy: true,
                    data: data.History,
                    "lengthChange": false,
                    "filter": false,
                    "columns": [
                        { "data": "RefNo", "autoWidth": true },
                        { "data": "TicketType", "autoWidth": true },
                        { "data": "RequestDate", "autoWidth": true },
                        { "data": "Employee", "autoWidth": true },
                        { "data": "EmployeeId", "autoWidth": true },
                        { "data": "TicketBalance", "autoWidth": true },
                        { "data": "Remarks", "autoWidth": true },
                        {
                            render: function (data, type, row) {

                                var html = '<button type="button" onclick="StatusBinding(' + row.RefNo + ')" class="btn btn-sm btn-outline-secondary" title="Open"><i class="fa fa-list"></i></button>   <button type="button" onclick=Download(' + row.AttachmentGuid + ') class="btn btn-sm btn-outline-secondary" title="Attachement"><i class="fa fa-paperclip"></i></button>';
                                //if (row.IsEditViisible == true) {
                                //    html += ' <button type="button" onclick="ReimbursementEditDataBinding('+row.Ref+')" class="btn btn-sm btn-outline-secondary" title="Edit"><i class="fa fa-pencil"></i></button>';
                                //}
                                if (row.isSubmitVisible == true) {
                                    html += ' <button type="button" class="btn btn-sm btn-outline-secondary" title="Submit Now!"><i class="fa fa-check"></i></button>';
                                }
                                if (row.isInProcessVisible == true) {
                                    html += ' <button type="button"  class="btn btn-sm btn-outline-secondary" title="Request is in Process"><i class="fa fa-cog"></i></button>';
                                }
                                if (row.isRecallVisible == true) {
                                    html += ' <button type="button" onclick="EditDataBinding(' + row.RefNo + ')" class="btn btn-sm btn-outline-secondary" title="Recall Process"><i class="fa fa-refresh"></i></button>';
                                }
                                if (row.isCompletedVisible == true) {
                                    html += ' <button type="button" class="btn btn-sm btn-outline-secondary" title="Request has been Complete"><i class="fa fa-save"></i></button>';
                                }
                                return html;                                    //< label class="label-100" id = "Cycle_' + row.ScheduleId + '" > ' + row.CycleTime + '</label >
                            }
                        },

                    ]

                });
                if (data.IsApproval == true) {
                    $("#Approvalhidden").prop("hidden", false);
                    $("#ApprovalTicketList").DataTable({
                        "lengthChange": true,
                        "pageLength": 25,
                        destroy: true,
                        data: data.Approval,
                        "lengthChange": false,
                        "filter": false,
                        "columns": [
                            { "data": "RefNo", "autoWidth": true },
                            { "data": "TicketType", "autoWidth": true },
                            { "data": "RequestDate", "autoWidth": true },
                            { "data": "Employee", "autoWidth": true },
                            { "data": "EmployeeId", "autoWidth": true },
                            { "data": "TicketBalance", "autoWidth": true },
                            { "data": "CompanyName", "autoWidth": true },
                            { "data": "Remarks", "autoWidth": true },
                            {
                                render: function (data, type, row) {
                                    var html = '<button type="button" class="btn btn-sm btn-outline-secondary" title="Attachement"><i class="fa fa-paperclip"></i></button>';
                                    html += ' <button onclick="ShowApprove_RejectModal(' + row.RefNo + ',' + row.ReqStatus + ')"  type="button" class="btn btn-sm btn-outline-secondary" title="Approve or Reject"><i class="fa fa-check"></i></button>';
                                    return html;
                                    //< label class="label-100" id = "Cycle_' + row.ScheduleId + '" > ' + row.CycleTime + '</label >
                                }
                            },
                        ]
                    });
                }
            }
        });

    }

    function StatusBinding(id)
    {
        $.ajax({
            type: "Post",
            url: "/Request/GetTicketDetails",
            data: { Id: id },
            success: function (data) {
                $("#StatusTicketModal").modal();

                $("#DetailList").DataTable({
                    "lengthChange": false,
                    "pageLength": 25,
                    destroy: true,
                    data: data,
                    "filter": false,
                    "columns": [
                        { "data": "RequestDate", "autoWidth": true },
                        { "data": "ReqStatus", "autoWidth": true },
                        { "data": "Description", "autoWidth": true },
                        { "data": "Approver", "autoWidth": true },
                        { "data": "ApprovedBy", "autoWidth": true },
                        { "data": "Remarks", "autoWidth": true },
                    ]
                });
            }

        });
    }

    function Save()
    {
        var Emp = $("#User option:selected").val();
        var Type = $("#TravelType option:selected").val();
        var Remarks = $("#Remarks").val();
        var FromCountry = $("#FromCountry option:selected").val();
        var FromCity = $("#TravelFromCity option:selected").val();
        var ToCountry = $("#ToCountry option:selected").val();
        var ToCity = $("#TravelToCity option:selected").val();
        var TravelDate = $("#TravelDate").val();
        var ReturnDate = $("#ReturnDate").val();
        var TicketBalance = "";
         var data = new FormData();
        var files = $("#file").get(0).files;
        if (files.length > 0) {
            data.append("HelpSectionImages", files[0]);
            data.append("FormName", "TicketRequest");
           $.ajax({
                url: '/Request/SaveProfileImage',
                type: "POST",
                processData: false,
                data: data,
                dataType: 'json',
                contentType: false,
                success: function (response) {
                    if (response != "Error While Saving." || response != 'No File Saved.')
        $.ajax({
            type: 'POST',
            url: '@Url.Action("SaveTicket", "Request")',
            data: { AtchGuid: response, TicketBalance: TicketBalance, Emp: Emp, Type: Type, Remarks: Remarks, FromCountry: FromCountry, FromCity: FromCity, ToCountry: ToCountry, ToCity: ToCity, TravelDate: TravelDate, ReturnDate: ReturnDate },
            success: function (data) {
                if (data=="") {
                    BindDataTable();
                }
                else {
                    alert(data);
                }
                location.reload();

            },
            error: function (data)
            {
            }
        });
                    $("#file").val('');
                },
                error: function (er) { }

            });
        }
        else {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("SaveTicket", "Request")',
