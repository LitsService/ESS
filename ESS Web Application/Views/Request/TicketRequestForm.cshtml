@{
    ViewBag.Title = "TicketRequestForm";
}
<div class="block-header">
    <div class="row">
        <div class="col-lg-6 col-md-8 col-sm-12">
            <h2><a href="javascript:void(0);" class="btn btn-xs btn-link btn-toggle-fullwidth"><i class="fa fa-arrow-left"></i></a> Dashboard</h2>
            <ul class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html"><i class="icon-home"></i></a></li>
                <li class="breadcrumb-item active">Dashboard</li>
            </ul>
        </div>

    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12">
        <div class="card">
            <div class="header">
                <h2>Ticket Request Form</h2>
            </div>
            <div class="body">
                <ul class="nav nav-tabs">
                    <li class="nav-item"><a class="nav-link active show" data-toggle="tab" href="#New">New Application</a></li>
                    <li class="nav-item"><a class="nav-link" data-toggle="tab" href="#History">History</a></li>
                    <li class="nav-item" hidden id="Approvalhidden"><a class="nav-link" data-toggle="tab" href="#Approval">Approval</a></li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane show active" id="New">
                        <div class="row">
                            <div class="col-lg-8 col-md-7">
                                <div class="card">
                                    <div class="header">
                                        <div class="col-12">
                                            <div class="row form-group">
                                                <div class="col-md-1">
                                                </div>
                                                <div class="col-md-4">
                                                </div>
                                                <div class="col-md-2" style="margin-left:10%;">
                                                    <input type="file" hidden name="file" id="file" />
                                                    <button type="submit" onclick="Attachment()" class="btn btn-primary">Attachments</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="body">
                                        <div class="table-responsive">
                                            <div class="col-md-6 col-sm-12">
                                                <label>Employee</label>
                                                <div class="form-group">
                                                    <select id="User" onchange="GetEmployeeDetails(this)" class="form-control show-tick">
                                                        <option value="0">Select Employee</option>
                                                        @foreach (var item in ViewBag.User)
                                                        {
                                                            if (item.Text == "Self")
                                                            {
                                                                <option selected value="@item.Value">@item.Text</option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@item.Value">@item.Text</option>
                                                            }
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="form-group">
                                                    <label>Ticket Balance: </label>
                                                    <text>0</text>
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-sm-12">
                                                <label>Travel Type</label>
                                                <div class="form-group">
                                                    <select id="TravelType" class="form-control show-tick">
                                                        <option value="0">Select Type</option>
                                                        @foreach (var item in ViewBag.Type)
                                                        {
                                                            <option value="@item.Value">@item.Text</option>
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-12 col-sm-12">
                                                <label>Travel From</label>
                                                <div class="row">
                                                    <div class="form-group col-md-4">
                                                        <select id="FromCountry" onchange="TravelFromGetCity(this)" class="form-control show-tick">
                                                            <option value="0">Select Country</option>
                                                            @foreach (var item in ViewBag.Countries)
                                                            {
                                                                <option value="@item.Value">@item.Text</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <select id="TravelFromCity" class="col-md-6 form-control show-tick">
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12 col-sm-12">
                                                <label>Travel To</label>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <select id="ToCountry" onchange="TravelToGetCity(this)" class="form-control show-tick">
                                                            <option value="0">Select Country</option>
                                                            @foreach (var item in ViewBag.Countries)
                                                            {
                                                                <option value="@item.Value">@item.Text</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <select id="TravelToCity" class="col-md-6 form-control show-tick">
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="col-6">
                                                <label>Date of Travel</label>
                                                <div class="form-group">
                                                    <input id="TravelDate" type="text" data-provide="datepicker" data-date-autoclose="true" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <label>Date of Return</label>
                                                <div class="form-group">
                                                    <input id="ReturnDate" type="text" data-provide="datepicker" data-date-autoclose="true" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <label>Remarks</label>
                                                <div class="form-group">
                                                    <textarea id="Remarks" rows="6" class="form-control no-resize" placeholder="Remarks"></textarea>
                                                </div>
                                            </div>
                                            <br>
                                            <button onclick="Save()" type="submit" class="btn btn-primary">Submit</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-5">
                                <div class="card">
                                    <div class="header">
                                        <h2>Employee Detail Card</h2>
                                    </div>
                                    <div class="body text-center">
                                        <div class="chart easy-pie-chart-1" data-percent="75"> <span><img src="~/Content/assets/images/sm/user.jpg" alt="user" class="rounded-circle" /></span> </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Employee ID: </label>
                                                <text id="EmpID">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Name: </label>
                                                <text id="EmpName">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Designation: </label>
                                                <text id="EmpDesg">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Department: </label>
                                                <text id="EmpDept">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Nationality</label>
                                                <text id="EmpNation">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Address: </label>
                                                <text id="EmpAdd">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Phone: </label>
                                                <text id="EmpPhone">0</text>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label>Email: </label>
                                                <text id="EmpEmail">0</text>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="History">
                        <div class="row clearfix">
                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="body">
                                        <div class="table-responsive">
                                            <table id="TicketHistory" class="table table-bordered table-hover table-striped" cellspacing="0">
                                                <thead class="thead-dark">
                                                    <tr>
                                                        <th>Ref#</th>
                                                        <th>Ticket Type</th>
                                                        <th>Request Date</th>
                                                        <th>Employee</th>
                                                        <th>Employee ID</th>
                                                        <th>Ticket Balance</th>
                                                        <th>Remarks</th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="Approval">
                        <div class="row clearfix">
                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="body">
                                        <div class="table-responsive">
                                            <table id="ApprovalTicketList" class="table table-bordered table-hover table-striped" cellspacing="0">
                                                <thead class="thead-dark">
                                                    <tr>
                                                        <th>Ref#</th>
                                                        <th>Ticket Type</th>
                                                        <th>Request Date</th>
                                                        <th>Employee</th>
                                                        <th>Employee ID</th>
                                                        <th>Ticket Balance</th>
                                                        <th>Company</th>
                                                        <th>Remarks</th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
