    function TravelFromGetCityEdit(e) {
        var countryId = e.value;
        $("#TravelFromCityEdit").html("");
        $.ajax({
            type: "Post",
            url: "/Request/GetCities",
            data: { CountryId: countryId },
            success: function (data) {
                var Cities = '';
                for (var i = 0; i < data.length; i++) {
                    Cities += '<option value="' + data[i].Value + '">' + data[i].Text + '</option>';
                }
                $("#TravelFromCityEdit").html(Cities);
            }
        });
    }

